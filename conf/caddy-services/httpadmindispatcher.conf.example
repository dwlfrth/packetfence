{
	admin off
	log {
		output stdout
		format transform "{common_log}"
	}
	auto_https off
}

:8890 {
	route {
		logger {
			level INFO
		}

		httpdportalpreview
	}
}

:8891 {
    handle_path /static/doc/* {
		root * /usr/local/pf/docs
        file_server
	}

    handle_path /static/* {
		root * /usr/local/pf/html/pfappserver/root
        file_server
	}

    handle_path /common/* {
		root * /usr/local/pf/html/common
        file_server
	}

    handle_path /admin/* {
		root * /usr/local/pf/html/pfappserver/root/dist
        file_server
    }
}
